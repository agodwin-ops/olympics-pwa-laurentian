{
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://olympics-pwa-laurentian.onrender.com"
  },
  "functions": {
    "apps/web/**": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://olympics-pwa-laurentian.onrender.com/api/$1"
    }
  ]
}